<h1>🔗 Backend Connectivity Check</h1>
<pre id="response-box">⏳ Contacting /list...</pre>

<script>
  fetch("/list")
    .then(res => res.json())
    .then(data => {
      document.getElementById("response-box").textContent = JSON.stringify(data, null, 2);
    })
    .catch(err => {
      document.getElementById("response-box").textContent = "⚠️ Error contacting backend: " + err.message;
    });
</script>